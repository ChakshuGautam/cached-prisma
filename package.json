{
  "name": "cached-prisma",
  "version": "0.1.0",
  "description": "A Prisma client abstraction that simplifies caching.",
  "repository": "https://github.com/JoelLefkowitz/cached-prisma.git",
  "author": "Joel Lefkowitz",
  "license": "MIT",
  "keywords": [
    "Prisma",
    "Memcached",
    "cache",
    "client",
    "singleton"
  ],
  "scripts": {
    "cspell": "cspell '.*' '*' '**/*'",
    "eslint": "eslint 'src/**/*.ts' --fix",
    "prettier": "prettier . --write --ignore-path .gitignore",
    "remark": "remark -r .remarkrc -i .gitignore . .github",
    "build": "tsc -p src/tsconfig.json",
    "lint": "yarn cspell && yarn eslint && yarn remark",
    "format": "yarn prettier",
    "test": "yarn build && nyc mocha dist --exit"
  },
  "dependencies": {
    "@prisma/client": "^2.27.0",
    "memcached": "^2.2.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/memcached": "^2.2.7",
    "@types/mocha": "^8.2.3",
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "chai": "^4.3.4",
    "cspell": "^5.6.6",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-watch": "^7.0.0",
    "grunt": "^1.4.1",
    "grunt-cli": "^1.4.3",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-env": "^1.0.1",
    "grunt-exec": "^3.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^9.0.2",
    "mocha-param": "^2.0.1",
    "mochawesome": "^6.2.2",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "source-map-support": "^0.5.19",
    "typescript": "^4.3.5"
  }
}
